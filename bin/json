#! /usr/bin/env python3

import  json
import  sys


if len(sys.argv) > 1:
    input = open(sys.argv[1], 'r')
else:
    input = sys.stdin

first = True
for line in input:
    line = line.strip()
    if not line:
        continue
    try:
        obj = json.loads(line)
    except json.JSONDecodeError:
        continue
    if not first:
        print('')
    else:
        first = False
    print(json.dumps(obj, indent=4))

if input is not sys.stdin:
    input.close()

# vim:set et sw=4 ts=4:
